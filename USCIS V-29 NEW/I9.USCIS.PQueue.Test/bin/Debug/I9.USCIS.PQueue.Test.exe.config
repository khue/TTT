    <!-- application failure notification -->
    <add key="Failure.Email.From" value="I9 Postmaster &lt;DoNotReply@Fragomen.com&gt;"/>
    <add key="Failure.Email.SendTo" value="khue@fragomen.com"/>
    <add key="Failure.Email.Subject" value="Failure: USCIS Password Queue (Test System)"/>
    <add key="Failure.Email.Message" value="There has been an error/failure during the process.  The excetpion details should be listed below:"/>

  </appSettings>

